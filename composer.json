{
    "name": "promorepublic/google-my-business-api-client",
    "description": "Api client for google my business",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ihor K.",
            "email": "lov3catch@gmail.com"
        }
    ],
    "repositories": [
